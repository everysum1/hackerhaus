
<h1>Searches#show</h1>
<p>Find me in app/views/searches/show.html.erb</p>

<div class="page-header">
  <h1>Search Results</h1>
</div>

  <table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>City</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    <% @properties.each do |p| %>
    <tr>
      <td>
        <%= p.title %>
      </td>
      <td>
        <%= p.description %>
      </td>
      <td>
        <%= p.city_name %>
      </td>
      <td>
        <%= p.price %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
  
<div class="map-container">
  <div class="map">
    
  </div>
</div>
